[user]
    name = Xitong Gao
    email = gxtfmx@gmail.com
[alias]
    cm          = commit
    s           = status
    st          = stash
    sm          = submodule
    br          = branch
    rb          = rebase
    un          = reset
    unstage     = reset
    unstage-all = reset --soft HEAD^
    prune-all   = !git remote | xargs -n 1 git remote prune
    lg          = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    stats       = shortlog -s -n --no-merges
    pl          = pull origin master
    ps          = push origin master
[core]
    excludesfile = /Users/admko/.gitignore
    autocrlf     = input
    editor       = /usr/bin/vim
[color]
    ui          = true
    diff        = auto
    status      = auto
    branch      = auto
    interactive = auto
    grep        = auto
[branch]
    autosetupmerge  = always
    autosetuprebase = always
[merge]
    log     = true
    summary = true
