from shutil import which as _which


_conda_path = _which("conda")


if _conda_path:
    import sys as _sys
    from types import ModuleType as _ModuleType
    _mod = _ModuleType('xontrib.conda', "Autogenerated from $XONSH_SOURCE.")
    __xonsh__.execer.exec(
        $(_conda_path "shell.xonsh" "hook"),
        glbs=_mod.__dict__,
        filename="$(_conda_path shell.xonsh hook)")
    _sys.modules["xontrib.conda"] = _mod
    del _sys, _mod, _ModuleType
