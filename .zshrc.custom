chromium='/Applications/Google\ Chrome\ Canary.app/Contents/MacOS/Google\ Chrome\ Canary'
alias chromium='$chromium'
proxy-browser() {
    local host="$1"
    parallel --ungroup ::: "ssh -vv -N -T -o ControlMaster=no -D 1080 $host" "$chromium --proxy-server=socks5://localhost:1080 > /dev/null"
}

HTTPS_PROXY="127.0.0.1:7890"
SOCKS_PROXY="127.0.0.1:7890"
alias proxy="https_proxy=http://$HTTPS_PROXY http_proxy=http://$HTTPS_PROXY all_proxy=socks5://$SOCKS_PROXY"
